<%= render 'refinery/admin/search_header', :url => refinery.members_admin_members_path %>
<!-- <div class='pagination_container'> -->
  <% if @members.any? %>
    <%= render 'members' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
<!-- </div> -->
